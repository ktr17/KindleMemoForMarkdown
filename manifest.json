{
  "name": "kindle Memo",
  "description": "Kindle Memo",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "matches": [
        "https://read.amazon.co.jp/notebook"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}